<?xml version="1.0" encoding="utf-8"?>
<ui ns="a">
	<window clazz="alz.mui.Window" title="窗体">
		<div class="ui-window-win2k" dock="false">
			<div class="win-head">
				<span class="icon"></span>
				<label>{$caption}</label>
				<div class="sysbtn">
					<a class="icon-min" href="#" title="最小化" _action="win_min"></a>
					<a class="icon-max" href="#" title="最大化" _action="win_max"></a>
					<a class="icon-close" href="#" title="关闭" _action="win_close"></a>
				</div>
			</div>
			<div class="win-body" container="true"></div>
		</div>
	</window>
	<toolwin clazz="alz.mui.ToolWin" title="工具窗体">
		<div class="ui-toolwin">
			<div class="win-head"><a class="icon-close" href="#" _action="win_close"></a><label>{$caption}</label></div>
			<div class="win-body" container="true"></div>
		</div>
	</toolwin>
	<dialog clazz="alz.mui.Dialog" title="对话框">
		<div class="ui-dialog">
			<div class="skin"></div>
			<h1 class="win-head"><label>{$caption}</label><a class="icon-close" href="#" _action="dlg_cancel"></a></h1>
			<div class="win-body" container="true"></div>
		</div>
	</dialog>
	<popup clazz="alz.mui.Popup">
		<div class="ui-popup" container="true"></div>
	</popup>
	<popmenu clazz="alz.mui.PopupMenu" title="弹出式菜单">
		<div class="ui-popmenu">
			<ul container="true"></ul>
		</div>
	</popmenu>
	<rebar>
		<div class="ui-rebar" container="true"></div>
	</rebar>
	<menubar clazz="alz.mui.MenuBar" title="菜单栏">
		<ul class="ui-menubar" container="true"></ul>
	</menubar>
	<menubutton class="alz.mui.MenuButton">
		<li class="ui-menubutton" _action="pop_show" _id="{$_id}">{$text}(<u>{$key}</u>)</li>
	</menubutton>
	<menuitem clazz="alz.mui.MenuItem">
		<li><a href="#" _action="{$action}"><!--{$text}(<u>{$key}</u>){$more}--></a></li>
	</menuitem>
	<menusep>
		<li class="sep"></li>
	</menusep>
	<toolbar clazz="alz.mui.ToolBar" title="工具栏">
		<ul class="ui-toolbar" container="true"><li class="hdl"></li></ul>
	</toolbar>
	<statusbar title="状态栏">
		<div class="ui-statusbar">
			<ul class="grids">
				<li class="grid" style="width:56px;">行 48</li>
				<li class="grid" style="width:44px;">列 27</li>
				<li class="grid" style="width:41px;">52</li>
				<li class="grid" style="width:29px;">EF</li>
				<li class="grid" style="width:46px;">UNIX,U8</li>
				<li class="grid" style="width:25px;">REC</li>
				<li class="grid" style="width:22px;">INS</li>
				<li class="grid" style="width:32px;color:#808080;">READ</li>
			</ul>
			<div class="text">要获得帮助, 请按[F1]键</div>
		</div>
	</statusbar>
	<hsplit title="水平分隔条">
		<div class="ui-slider-h"></div>
	</hsplit>
	<vsplit title="垂直分隔条">
		<div class="ui-slider-v"></div>
	</vsplit>
	<workspace clazz="alz.mui.AppWorkspace">
		<div class="ui-appworkspace" container="true"></div>
	</workspace>
	<pane clazz="alz.mui.Pane">
		<div class="ui-pane" container="true"></div>
	</pane>
	<panel clazz="alz.mui.Panel">
		<div class="ui-panel" container="true"></div>
	</panel>
	<dockpanel>
		<panel container="true"></panel>
	</dockpanel>
	<codeeditor clazz="alz.mui.CodeEditor">
		<textarea></textarea>
	</codeeditor>
